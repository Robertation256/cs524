mod MEMCACHE-TEST is 
    protecting MEMCACHE .
    protecting CONFIGURATION .

    op testConfig : -> Configuration .

    op data : -> Map{String, String} .

    eq data =
        insert ("a", "A", 
            insert("b", "B", 
                insert("c", "C", empty)
